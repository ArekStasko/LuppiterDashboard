<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        x:Class="LuppiterDashboard.MainWindow"
        Title="LuppiterDashboard"
        Width="800" Height="450"
        xmlns:local="clr-namespace:LuppiterDashboard"
        x:DataType="local:MainWindow">

    <StackPanel Margin="10">
        <TextBlock Text="Odebrane dane z ESP32:" FontSize="16" Margin="0,0,0,10"/>
        <ListBox ItemsSource="{Binding Data}" Height="350"/>
    </StackPanel>
</Window>